echo "Token-specific features:"
echo "	pkcsccamigrate build:	$enable_pkcscca_migrate"
echo "	pkcsep11migrate build:	$enable_pkcsep11_migrate"
echo
echo "CFLAGS=$CFLAGS"
echo
