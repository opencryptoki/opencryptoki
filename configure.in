          usr/lib/pkcs11/icsf_stdll/Makefile \
          misc/Makefile \
	  testcases/Makefile \
	  testcases/common/Makefile \
	  testcases/crypto/Makefile \
	  testcases/pkcs11/Makefile \
	  testcases/login/Makefile \
	  testcases/misc_tests/Makefile \
	  man/Makefile \
	  man/man1/Makefile \
	  man/man1/pkcsconf.1 \
	  man/man1/pkcsicsf.1 \
	  man/man1/pkcscca.1 \
	  man/man1/pkcsep11_migrate.1 \
